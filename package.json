    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "express-rate-limit": "^3.1.1",
    "moment": "^2.22.2",
    "mongoose": "^5.2.14",
    "pino": "^4.15.0",
    "rate-limit-redis": "^1.5.0",
    "redis": "^2.8.0",
    "source-map-support": "^0.5.9",
    "swagger-express-middleware": "^1.0.0-alpha.12",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
